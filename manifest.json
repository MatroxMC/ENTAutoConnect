{
  "name": "ENTAutoConnect",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "",
  "icons": {
    "16": "icons/ENTAutoConnect.png",
    "48": "icons/ENTAutoConnect.png",
    "128": "icons/ENTAutoConnect.png"
  },
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "js/eventPage.js"
  },
  "host_permissions": [
    "*://*/*"
  ]
}