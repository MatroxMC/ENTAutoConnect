{
  "name": "Better ENT",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "This is a browser extension that makes it easier connect with your ENT.",
  "icons": {
    "16": "icons/BetterENT.png",
    "48": "icons/BetterENT.png",
    "128": "icons/BetterENT.png"
  },
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "js/eventPage.js"
  },
  "host_permissions": [
    "*://*/*"
  ]
}